@model IEnumerable<WebsiteBanHang.Models.SanPham>

<div class="content-top-bottom">
    <h2>Laptop mới nhất</h2>
    <div class="col-md-6 men">
        <a href="#" class="b-link-stripe b-animate-go  thickbox">
            <img class="img-responsive" src="~/Content/images/@Model.ElementAt(0).HinhAnh" alt="">
            <div class="b-wrapper">
                <h3 class="b-animate b-from-top top-in   b-delay03 " style="text-align:left;line-height:25px;padding:5px">
                    @Model.ElementAt(0).TenSP<br />
                    @Html.Raw(Model.ElementAt(0).CauHinh)
                </h3>
            </div>
        </a>


    </div>
    <div class="col-md-6">
        <div class="col-md1 ">
            <a href="single.html" class="b-link-stripe b-animate-go  thickbox">
                <img class="img-responsive" src="~/Content/images/@Model.ElementAt(1).HinhAnh" alt="">
                <div class="b-wrapper">
                    <h3 class="b-animate b-from-top top-in1   b-delay03 "style="text-align:left;line-height:25px;padding:5px">
                        @Model.ElementAt(1).TenSP<br />
                        @Html.Raw(Model.ElementAt(1).CauHinh)
                    </h3>
                </div>
            </a>
        </div>
        <div class="col-md2">
            <div class="col-md-6 men1">
                <a href="single.html" class="b-link-stripe b-animate-go  thickbox">
                    <img class="img-responsive" src="@Model.ElementAt(2).HinhAnh" alt="">

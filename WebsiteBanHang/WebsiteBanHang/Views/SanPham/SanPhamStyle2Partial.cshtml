                        <h3 class="b-animate b-from-top top-in2   b-delay03 "style="text-align:left;line-height:25px;padding:5px">
                            @Model.ElementAt(1).TenSP<br />
                            @Html.Raw(Model.ElementAt(1).CauHinh)
                        </h3>
                    </div>
                </a>

            </div>
            <div class="clearfix"> </div>
        </div>
    </div>
    <div class="clearfix"> </div>
</div>